        function fncsave(btnNum) {
         
            $('#hfNavBtnID')[0].value = $('#hfScrollNumberL1')[0].value + ":" + $('#hfScrollNumberL2')[0].value + ":" + btnNum + ":Navbtn" + btnNum;
          // alert($('#hfNavBtnID')[0].value)
