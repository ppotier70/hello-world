        function businessRule(showMsg) {   //--- for ISP_ImportantThings.aspx ---------------
            //   if ($('#hfRequestedCancel')[0].value) { return; } //--no reason to check if user canceled
            $(document).ready(function() {
			
			   // get this page number -------
			   var myObj =  JSON.parse($('#hfnavCntrl')[0].value) ;
		       var thisPg = myObj.thisPg
			   console.log('thisPg =' + thisPg);

                // hide all edit buttons where checkbox is not checked item e ---
                for (var gIndex = 1; gIndex < 5; gIndex++) {
                    var ObjID = 'cbTechEquip' + gIndex
                    var objPanel = 'btnEdit' + gIndex
                    if ($('#' + ObjID).is(":checked")) {
                        $("#" + objPanel).show();
                    } else {
                        $("#" + objPanel).hide();
                    }


                }


                var InsVal = "y";
                var Myerrbody = "";

                //     if ($('#hfRequestedCancel')[0].value) { return; } //--no reason to check if user canceled
				
				var endBlk = 1;
				if(thisPg == '4'){
					endBlk = 1
				}else {
					endBlk = 4
				}
				

                for (var iIndex = 1; iIndex <= endBlk; iIndex++) {
                    if ($("[id*='ddlTOS" + iIndex + "']").length) {           // does this block exist?
                        // -- TOS block exists --------
                        var Myerrmsg = "";
                        if ($("[id*='ddlTOS" + iIndex + "']").val() > 1) {     // is TOS > 0 ?

                            if ($("[id*='ddlFreq" + iIndex + "']").val() == '0') {                 // was Freq selected?
                                //  alert("Not checked-Freq");
                                Myerrmsg += " Frequency ** ";
                            }
                            if (!$("input:radio[id*='rblTraining" + iIndex + "']:checked").val()) {  // was a training radio button checked?
                                //  alert("Not checked-rb");
                                Myerrmsg += " Training ** ";
                            }
                            if ($("[id*='ddlRisk" + iIndex + "']").val() == '0') {              // was Risk level selected?
                                // alert("Not checked-RC");
                                Myerrmsg += " Risk Level **";
                            }
                            if (!$("input:checkbox[id*='cblRisk" + iIndex + "']:checked").val()) {  // --Checkbox list 
                                //  alert("Not checked-RCcb");
                                Myerrmsg += " Risk Checkboxes **";
                            }
                            var OtherOn = false
                            $("input:checkbox[id*='cblRisk" + iIndex + "']:checked").each(function() {  // -- Other check box ---
                                if ($(this).next().html() == "Other:") {
                                    if ($(this).val() == 'on' || $(this).val().length > 0) { OtherOn = true; }
                                    //  alert('-Other-');
                                }
                            });
                            if (OtherOn == true) {
                                if ($("[id*='tblRiskOther" + iIndex + "']").val().trim().length == 0) {  // -- Other Text box, is it empty
                                    //   alert("no Other content-rc");
                                    Myerrmsg += " Risk - Other: textbox **";
                                }

                            }
                            // this keeps the Next or Previous buttons from firing only if tos is selected but not completed
                            //   if (Myerrmsg.length != "") event.preventDefault();

                        } else {
                            if ($("[id*='ddlTOS" + iIndex + "']").val() == '0' && $("[id*='hfChildHide" + iIndex + "']")[0].value == 'show') {
                                Myerrmsg += "Must select Type of Support **";
                            }

                        }

                        //  ---- Preference section ----------------------
                        if (!$("input:checkbox[id*='cblPref" + iIndex + "']:checked").val()) {
                            // alert("Not checked-Prefcb");
                            Myerrmsg += " Preference Checkboxes **";
                        }
                        var OtherOn2 = false
                        $("input:checkbox[id*='cblPref" + iIndex + "']:checked").each(function() {
                            if ($(this).next().html() == "Other:") {
                                if ($(this).val() == 'on' || $(this).val().length > 0) { OtherOn2 = true; }
                                //  alert('-Other-')

                            }
                        });
                        if (OtherOn2 == true) {
                            if ($("[id*='tblPrefOther" + iIndex + "']").val().trim().length == 0) {
                                //   alert("no Other content-pref");
                                Myerrmsg += " Preference - Other: textbox **";
                            }
                        }
                        var ltr = $('#lblBlkTitleLtr' + iIndex)[0].innerHTML.substr(0, 1);
                        var pg = $('#hfThisPgID')[0].value
                        if (Myerrmsg.length == "") {
                            $('#lblBlkTitleLtr' + iIndex).css('color', 'black');
                            //--- track page level errors --------------
                            statTrackerUpdateV2(pg, '0', ltr, 'y');
                        } else {
                            $('#lblBlkTitleLtr' + iIndex).css('color', 'red');
                            Myerrbody = Myerrbody + "\n ( " + $("[id*='lblBlkTitle" + iIndex + "']").text() + " )" + Myerrmsg
                            //--- track page level errors --------------
                            //  alert(pg + ' ' + ltr);
                            statTrackerUpdateV2(pg, 'n', ltr, 'n');
                            // alert(Myerrmsg);
                        }
                    }  //----- end of does this block exist? ------
                }     //----- end of FOR loop ---------------------

                // ---------- generate error message if needed ---------------------------------
                $('#StatBlk2').css('background-color', '#99ff99');     //this is green

                if (Myerrbody.length != "") {

                    if (showMsg) {
                        Myerrbody = "Following are the missing items: \n" + Myerrbody
                        alert(Myerrbody);

                    }
                    $('#StatBlk2').css('background-color', '#ff6666');      // this is red
                    InsVal = "n";
                }
                // now update statTracker for entire form
                //   determine which page we are in. hfThisPgID is set in the vb code
                var pg = $('#hfThisPgID')[0].value

                // alert($('#lblBlkTitleLtr1')[0].innerHTML)
                statTrackerUpdateV2(pg, InsVal, '0', '0');

                if (showMsg) return false;

            });
        }
    </script>

