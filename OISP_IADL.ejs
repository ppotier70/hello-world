		   myObj.nextFm = myObj.thisFm //-- point to same form --
		   
		  $('#hfnavCntrl')[0].value = JSON.stringify(myObj);
		   form.submit();
            }else {
                event.preventDefault();
				return false;
            }
         }
		
		function btnNext(){
		var myObj =  JSON.parse($('#hfnavCntrl')[0].value) ;
		 myObj.nextFm = myObj.thisFm //-- point to same form --
		 
		var maxPgCt = 0;
		 if(myObj.thisFm == "222A") {
		   maxPgCt = 4;
		  }
		  if (Number(myObj.thisPg) + 1 > maxPgCt){
		   myObj.nextPg = maxPgCt.toString()
		  }else{
		   myObj.nextPg = (Number(myObj.thisPg) + 1).toString()
		  }
		  myObj.FA = "S";
		 
