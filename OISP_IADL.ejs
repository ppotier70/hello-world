		 var myObj =  JSON.parse($('#hfnavCntrl')[0].value) ;
		 
		 
           if (confirm("\nAre you sure you want to cancel what you have entered on this form?")) {
          // ----establish value of NavCntrl, used if user hits Cancel. This is like a reload 
